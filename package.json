{
	"name": "vscode-pc-viewer",
	"displayName": "vscode-pc-viewer",
	"description": "Display points of a file.",
	"version": "0.1.0",
	"publisher": "obarads",
	"engines": {
		"vscode": "^1.44.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"point cloud",
		"viewer"
	],
	"icon": "logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Obarads/vscode-pc-viewer.git"
	},
	"activationEvents": [
		"onLanguage:pcd",
		"onLanguage:ply",
		"onWebviewPanel:pcviewer.panel"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "ply",
				"aliases": ["Polygon File Format","ply"],
				"extensions": [".ply"]
			},
			{
				"id": "pcd",
				"aliases": ["point cloud data","pcd"],
				"extensions": [".pcd"]
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.44.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.26.0",
		"@typescript-eslint/eslint-plugin": "^2.26.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	}
}
